<resources>
    <declare-styleable name="FileTreeView">
        <attr name="recyclerItemViewCount" format="integer"/>
        <attr name="recyclerItemViewEnabled" format="boolean"/>
        <attr name="itemViewCacheSize" format="integer"/>
        <attr name="itemViewCachingEnabled" format="boolean"/>
        <attr name="fileMapMaxSize" format="integer"/>
        <attr name="fileMapEnabled" format="boolean"/> 
        <attr name="fileTreeAnimation" format="enum">
            <enum name="FallDown" value="0"/>
            <enum name="RotateIn" value="1"/>
            <enum name="ScaleUp" value="2"/>
            <enum name="SlideIn" value="3"/>
            <enum name="Default" value="4"/>
        </attr>
        <attr name="fileTreeAnimationEnabled" format="boolean"/>
    </declare-styleable>
</resources>